# -*- coding: utf-8 -*-
# for initilize this tasks, you should type `bundler`
Rake.add_rakelib 'tasks'

# ref: http://bundler.io/
# ref: http://docs.seattlerb.org/rake/
# ref: http://cobwwweb.com/4-ways-to-pass-arguments-to-a-rake-task
# ref: http://ionrails.com/2009/09/19/ruby_require-vs-load-vs-include-vs-extend/
# ref: http://tech.natemurray.com/2007/03/ruby-shell-commands.html
task :default do
  sh 'rake -T'
end


task :help do
  puts <<EOS
hi
EOS
end
